
% FACT: program(program_id, program_name, qualified_program_name, begin_annotation_id, end_annotation_id).
program(1, 'get_prism_data', 'get_prism_data', 1, 7).
program(2, 'get_prism_data', 'get_prism_data.get_prism_data', 1, 7).

% FACT: workflow(program_id).
workflow(1).

% FACT: function(program_id).

% FACT: has_subprogram(program_id, subprogram_id).
has_subprogram(1, 2).

% FACT: port(port_id, port_type, port_name, qualified_port_name, port_annotation_id, data_id).
port(1, 'in', 'prism_gridded_data', 'get_prism_data<-prism_gridded_data', 3, 1).
port(2, 'out', 'prism_data', 'get_prism_data->prism_data', 5, 2).
port(3, 'in', 'prism_gridded_data', 'get_prism_data.get_prism_data<-prism_gridded_data', 3, 1).
port(4, 'out', 'prism_data', 'get_prism_data.get_prism_data->prism_data', 5, 2).

% FACT: port_alias(port_id, alias).

% FACT: port_uri_template(port_id, uri).
port_uri_template(1, 'http://www.prism.oregonstate.edu/historical').
port_uri_template(2, 'file:/data/{data_file}.nc').
port_uri_template(3, 'http://www.prism.oregonstate.edu/historical').
port_uri_template(4, 'file:/data/{data_file}.nc').

% FACT: has_in_port(block_id, port_id).
has_in_port(1, 1).
has_in_port(2, 3).

% FACT: has_out_port(block_id, port_id).
has_out_port(1, 2).
has_out_port(2, 4).

% FACT: data(data_id, data_name, qualified_data_name).
data(1, 'prism_gridded_data', '[prism_gridded_data]').
data(2, 'prism_data', '[prism_data]').

% FACT: channel(channel_id, data_id).
channel(1, 2).
channel(2, 1).

% FACT: port_connects_to_channel(port_id, channel_id).
port_connects_to_channel(4, 1).
port_connects_to_channel(3, 2).

% FACT: inflow_connects_to_channel(port_id, channel_id).
inflow_connects_to_channel(1, 2).

% FACT: outflow_connects_to_channel(port_id, channel_id).
outflow_connects_to_channel(2, 1).

% FACT: uri_variable(uri_variable_id, variable_name, port_id).
uri_variable(1, 'data_file', 2).
uri_variable(2, 'data_file', 4).

